                <paramer name="@DEFAULT_COLLATE_NAME" value="DEFAULT_COLLATE_NAME" script="DEFAULT_COLLATE_NAME"/>
                <paramer name="@DESCRIPTION" value="DESCRIPTION" script="DESCRIPTION"/>
                <paramer name="@MAXLEN" value="MAXLEN" script="MAXLEN"/>
            </Paramers>
        </SQL>

